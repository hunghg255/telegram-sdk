{
  "name": "telegram-sdk",
  "version": "0.0.3",
  "description": "Telegram Web App - (Uses July 7, 2024, Bot API 7.7)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/core.d.mts",
        "default": "./dist/core.mjs"
      },
      "require": {
        "types": "./dist/core.d.ts",
        "default": "./dist/core.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "prepublish": "npm run build",
    "release": "bumpp",
    "lint": "tsc",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunghg255/telegram-sdk.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "telegram-web-app",
    "telegram-mini-app",
    "telegram",
    "telegram-bot",
    "telegram web apps",
    "telegram mini apps",
    "telegram bot",
    "telegram bot api",
    "telegram-sdk"
  ],
  "author": "hunghg255",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hunghg255/telegram-sdk/issues"
  },
  "homepage": "https://github.com/hunghg255/telegram-sdk#readme",
  "devDependencies": {
    "bumpp": "^9.4.1",
    "git-scm-hooks": "^0.0.11",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "verify-commit-msg": "^0.0.14"
  },
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
